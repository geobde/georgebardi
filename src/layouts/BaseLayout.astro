---
import MainHead from '../components/MainHead.astro'
import Footer from '../components/Footer.astro'

const { title, description } = Astro.props
---

<html lang='en'>
  <head>
    <MainHead title={title} description={description} />
  </head>
  <body>
    <div class='stack'>
      <slot />
      <Footer />
    </div>
  </body>
</html>
